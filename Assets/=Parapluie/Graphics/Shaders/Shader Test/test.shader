			half4 c = 0;
			float4 color1 = IsGammaSpace() ? float4(0.3486873,1,0,0) : float4(0.09970164,1,0,0);
			float4 color9 = IsGammaSpace() ? float4(0,0.3944354,1,0) : float4(0,0.1290018,1,0);
			float3 ase_worldNormal = i.worldNormal;
			float3 ase_vertexNormal = mul( unity_WorldToObject, float4( ase_worldNormal, 0 ) );
